      },
      "outputs": [],
      "source": [
        "# load the plot function of rasterio\n",
        "from rasterio import plot as rplt\n",
        "%matplotlib inline\n",
        "# plot the georeferenced data\n",
        "rplt.show(DEM, title='DEM', cmap='pink')"
      ]
    },
